{
  "manifest_version": 3,
  "name": "Who A'int Following Me Back",
  "version": "1.0",
  "description": "See who isn’t following you back on Instagram™ by comparing your followers and following lists.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["content.js"]
    }
  ]
}